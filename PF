-- misc

local AutoVK = Misc:CreateToggle("Call random votekick (Rank 25+)", nil, function(State)
    if State then
        spawn(function()
            local players = game:GetService("Players"):GetPlayers()

            if #players > 0 then
                RandomPlayer = players[math.random(1, #players)]
            end

            if RandomPlayer.Name ~= LclplrName then
                AllowedRandom = true
            end

            if not Menu:isdeployed() then
                if AllowedRandom then
                    Network:send("spawn")
                    wait(0.3)
                    game:GetService("ControllerService").RemoteEvent:FireServer("modcmd", "/votekick:"..RandomPlayer.Name..":Exploiting")
                    task.wait(0.7)
                    for i, v in pairs(getnilinstances()) do
                        if v.ClassName == "ModuleScript" and v.Name == "network" then
                            for i = 1, 100 do
                                require(v):send("repupdate", Vector3.new(), Vector2.new(), tick())
                            end
                        end
                    end
                end
            end
            if Menu:isdeployed() then
                game:GetService("ControllerService").RemoteEvent:FireServer("modcmd", "/votekick:"..RandomPlayer.Name..":Exploiting")
            end
        end)
    end
end)

AutoVK:AddToolTip("Votekicks a random player for exploiting (Rank 25+ needed)")
AutoVK:CreateKeybind("J", function(key)end)

