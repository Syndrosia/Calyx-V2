-- INITIALIZATION IMPORT HERE

function initialize()
    spawn(function()
        local AR = game:GetService("ReplicatedStorage").Misc.Msger
        local b = AR:Clone()
        local a = AR:Clone()
        a.Parent = game:GetService("ReplicatedStorage").Misc
        b.Parent = game:GetService("ReplicatedStorage").Misc

        game:GetService("Players")[LclplrName].PlayerGui.ChatGame.Version.Text = "6.0.0 Modified with Calyx V2"
        game:GetService("Players")[LclplrName].PlayerGui.ChatGame.Version.TextSize = 20

        b.Parent = game:GetService("Players")[LclplrName].PlayerGui.ChatGame.GlobalChat
        b.Text = " $ Calyx V2 $"
        b.Msg.Text = "  Loading script..."
        b.Tag.Staff.Visible = true
        b.TextStrokeTransparency = 1
        b.Msg.TextStrokeTransparency = 1
        b.Tag.Staff.Image = "http://www.roblox.com/asset/?id=7197419712"

        a.Parent = game:GetService("Players")[LclplrName].PlayerGui.ChatGame.GlobalChat
        a.Text = " $ Calyx V2 $"
        a.Msg.Text = "  Successfully loaded! Took: "..math.random(480,1392).."ms"
        a.Tag.Staff.Visible = true
        a.TextStrokeTransparency = 1
        a.Msg.TextStrokeTransparency = 1
        a.Tag.Staff.Image = "http://www.roblox.com/asset/?id=7197419712"
        
        local count = 0

        for i,v in pairs (game:GetService("Players")[LclplrName].PlayerGui.ChatGame.GlobalChat:GetChildren()) do
            count = count + 1
        end

        if count > 7 then
            a.Text = ""
            b.Text = ""
            b.Parent = game:GetService("ReplicatedStorage").Misc
            a.Parent = game:GetService("ReplicatedStorage").Misc
        end

        while wait() do
            for i = 0,1,0.001*4 do
                a.TextColor3 = Color3.fromHSV(i,1,1)
                b.TextColor3 = Color3.fromHSV(i,1,1)
                game:GetService("Players")[LclplrName].PlayerGui.ChatGame.Version.TextColor3 = Color3.fromHSV(i,1,1)       
            end
        end
    end)
end

initialize()

