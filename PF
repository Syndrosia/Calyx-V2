local ChatSpam = Misc:CreateToggle("Chat Spam", nil, function(State)
    if State then
    _G.Pro = true
        spawn(function()
            if _G.Pro then
                loadstring(game:HttpGet('https://pastebin.com/raw/BApwJN7p', true))()
            end
        end)
    elseif not State then
        _G.Pro = false
    end
end)

ChatSpam:AddToolTip("Spams chat with emojis and toxicity")

local AutoVote = Misc:CreateToggle("Auto vote", true, function(State)
    if State then
        spawn(function()
            local name = game:GetService("Players").LocalPlayer.Name
            local vk = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("ChatGame").Votekick
            vk:GetPropertyChangedSignal("Visible"):Connect(function()
                if vk.Title.Text == "Votekick "..name.." out of the server?" then
                    keypress(0x4E)   
                    wait(0.03)
                    keyrelease(0x4E)
                else
                    if getgenv().vkoptionselected == "Yes" then
                        keypress(0x59)
                        wait(0.01)
                        keyrelease(0x59)
                    elseif getgenv().vkoptionselected == "Dismiss" then
                        keypress(0x4A)
                        wait(0.01)
                        keyrelease(0x4A)
                    elseif getgenv().vkoptionselected == "No" then
                        keypress(0x4E)   
                        wait(0.03)
                        keyrelease(0x4E)
                    end
                end
            end)
        end)
    end
end)

AutoVote:AddToolTip("Automatically votes when votekick is avalible (Votes no for you)")

local autovkoption = Misc:CreateDropdown("Votekick option", {"No", "Dismiss", "Yes"}, function(String)
    getgenv().vkoptionselected = String
end)

autovkoption:AddToolTip("Votekick option sent when the votekick is avalible (You are automatically no)")
autovkoption:SetOption("Yes")

