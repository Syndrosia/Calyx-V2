-- settings

local UIToggle = UI:CreateToggle("UI Toggle", nil, function(State)
    Window:Toggle(State)
end)

UIToggle:AddToolTip("Toggles the UI")

UIToggle:CreateKeybind(tostring(Config.Keybind):gsub("Enum.KeyCode.", ""), function(Key)
    Config.Keybind = Enum.KeyCode[Key]
end)

UIToggle:SetState(true)

local UIColour = UI:CreateColorpicker("UI Color", function(Color)
    Window:ChangeColor(Color)
end)

UIColour:AddToolTip("Colour of the UI")
UIColour:UpdateColor(Config.Color)

local BackgroundImage = UI:CreateDropdown("Image", {"Topography","Hearts","Abstract","Hexagon","Circles","Lace With Flowers","Floral"}, function(Name)
    if Name == "Default" then
        Window:SetBackground("2151741365")
    elseif Name == "Hearts" then
        Window:SetBackground("6073763717")
    elseif Name == "Abstract" then
        Window:SetBackground("6073743871")
    elseif Name == "Hexagon" then
        Window:SetBackground("6073628839")
    elseif Name == "Circles" then
        Window:SetBackground("6071579801")
    elseif Name == "Lace With Flowers" then
        Window:SetBackground("6071575925")
    elseif Name == "Floral" then
        Window:SetBackground("5553946656")
    end
end)

BackgroundImage:SetOption("Topography")
BackgroundImage:AddToolTip("Background image for the UI")

local BackgroundColour = UI:CreateColorpicker("Color", function(Color)
    Window:SetBackgroundColor(Color)
end)

BackgroundColour:UpdateColor(Color3.new(1,1,1))
BackgroundColour:AddToolTip("The background colour for the UI")

local UITransparency = UI:CreateSlider("Transparency", 0, 1, 0, false, function(Value)
    Window:SetBackgroundTransparency(Value)
end)

UITransparency:AddToolTip("The transparency for the UI")

local TileScale = UI:CreateSlider("Tile Scale", 0, 1, nil, false, function(Value)
    Window:SetTileScale(Value)
end)

TileScale:SetValue(0.5)
TileScale:AddToolTip("The scale for the background tile")

end -- dont remove (if gamename == 292........ then load pf)
